@{
  Layout = "_Layout";
}

<div class="container">

<h1> List of all doctors</h1>
<ol>
  @foreach (var doctor in Model)
  {
    <li><a href="/doctors/@doctor.GetId()">@doctor.GetName()</a></li>
  }
</ol>
  <h3><a href='/doctors/new'>Add a new doctor</a></h3>
  <h3><a href="/">Go Back Home</a></h3>
</div>
